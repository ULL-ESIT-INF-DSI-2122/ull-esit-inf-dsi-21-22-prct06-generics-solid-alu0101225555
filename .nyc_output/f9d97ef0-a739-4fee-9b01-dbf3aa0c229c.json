.vectorPokemon);\n    }\n    \n}"]},"_coverageSchema":"1a1c01bbd47fc00a2c39e90264f33305004495a9","hash":"f68503c9e4343b3e2e40fa882a7a34331ebccfe6","contentHash":"fefa85f5a8d1154901c562b7f61583e7a1d11694f58dc74a45a8de6f4e60a6f3"},"/home/usuario/p6/src/ejercicio-1/combat.ts":{"path":"/home/usuario/p6/src/ejercicio-1/combat.ts","statementMap":{"0":{"start":{"line":2,"column":0},"end":{"line":2,"column":62}},"1":{"start":{"line":3,"column":0},"end":{"line":3,"column":24}},"2":{"start":{"line":9,"column":8},"end":{"line":9,"column":25}},"3":{"start":{"line":10,"column":8},"end":{"line":10,"column":25}},"4":{"start":{"line":11,"column":8},"end":{"line":11,"column":25}},"5":{"start":{"line":12,"column":8},"end":{"line":12,"column":25}},"6":{"start":{"line":24,"column":29},"end":{"line":24,"column":30}},"7":{"start":{"line":25,"column":22},"end":{"line":25,"column":23}},"8":{"start":{"line":26,"column":28},"end":{"line":26,"column":31}},"9":{"start":{"line":27,"column":26},"end":{"line":27,"column":27}},"10":{"start":{"line":28,"column":20},"end":{"line":28,"column":21}},"11":{"start":{"line":29,"column":8},"end":{"line":31,"column":9}},"12":{"start":{"line":30,"column":12},"end":{"line":30,"column":40}},"13":{"start":{"line":32,"column":8},"end":{"line":39,"column":9}},"14":{"start":{"line":33,"column":12},"end":{"line":34,"column":44}},"15":{"start":{"line":34,"column":16},"end":{"line":34,"column":44}},"16":{"start":{"line":35,"column":12},"end":{"line":36,"column":45}},"17":{"start":{"line":36,"column":16},"end":{"line":36,"column":45}},"18":{"start":{"line":37,"column":12},"end":{"line":38,"column":38}},"19":{"start":{"line":38,"column":16},"end":{"line":38,"column":38}},"20":{"start":{"line":40,"column":8},"end":{"line":47,"column":9}},"21":{"start":{"line":41,"column":12},"end":{"line":42,"column":45}},"22":{"start":{"line":42,"column":16},"end":{"line":42,"column":45}},"23":{"start":{"line":43,"column":12},"end":{"line":44,"column":44}},"24":{"start":{"line":44,"col