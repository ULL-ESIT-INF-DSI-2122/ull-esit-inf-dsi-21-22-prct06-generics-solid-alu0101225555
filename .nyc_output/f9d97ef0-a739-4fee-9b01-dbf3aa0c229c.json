  * \n     * @returns velocidad del pokemon\n     */\n    getVelocidad() {\n        return this.velocidad;\n    }\n\n    /**\n     * \n     * @returns puntos daño máximo del pokemon\n     */\n    getHp() {\n        return this.hp;\n    }\n\n    /**\n     * Método que modifica el valor HP de un pokemon\n     * @param hp nuevo valor HP \n     */\n    setHP(hp: number) {\n        this.hp = hp;\n    }\n}\n"]},"_coverageSchema":"1a1c01bbd47fc00a2c39e90264f33305004495a9","hash":"314ae7759d538d9b9f42dbf2e596979f18b5dd7b","contentHash":"62577de69b46172b6553b7c9c72582faa690593252e8f07607f7adb56bb798c4"},"/home/usuario/p6/src/ejercicio-1/pokedex.ts":{"path":"/home/usuario/p6/src/ejercicio-1/pokedex.ts","statementMap":{"0":{"start":{"line":2,"column":0},"end":{"line":2,"column":62}},"1":{"start":{"line":3,"column":0},"end":{"line":3,"column":25}},"2":{"start":{"line":9,"column":8},"end":{"line":9,"column":43}},"3":{"start":{"line":10,"column":8},"end":{"line":10,"column":43}},"4":{"start":{"line":17,"column":8},"end":{"line":17,"column":34}},"5":{"start":{"line":24,"column":8},"end":{"line":24,"column":37}},"6":{"start":{"line":32,"column":8},"end":{"line":32,"column":48}},"7":{"start":{"line":38,"column":8},"end":{"line":38,"column":40}},"8":{"start":{"line":41,"column":0},"end":{"line":41,"column":26}}},"fnMap":{"0":{"name":"(anonymous_0)","decl":{"start":{"line":8,"column":4},"end":{"line":8,"column":5}},"loc":{"start":{"line":8,"column":31},"end":{"line":11,"column":5}},"line":8},"1":{"name":"(anonymous_1)","decl":{"start":{"line":16,"column":4},"end":{"line":16,"column":5}},"loc":{"start":{"line":16,"column":23},"end":{"line":18,"column":5}},"line":16},"2":{"name":"(anonymous_2)","decl":{"start":{"line":23,"column":4},"end":{"line":23,"column":5}},"loc":{"start":{"line":23,"column":20},"end":{"line":25,"column":5}},"line":23},"3":{"name":"(anonymous_3)","decl":{"start":{"line":31,"column":4},"end":{"line":31,"column":5}},"loc":{"start":{"line":31,"column":21},"end":{"line